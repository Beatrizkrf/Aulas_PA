        "#Digitados pelo usuário\n",
        "\n",
        "inicio = int(input('Digite o primeiro número:' ))\n",
        "fim = int(input('Digite o primeiro número:' ))\n",
        "\n",
        "if inicio>fim:\n",
        "  #aux = inicio\n",
        "  #inicio = fim\n",
        "  #fim= aux\n",
        "\n",
        "  inicio, fim = fim,inicio\n",
        "\n",
        "for i in range(inicio,fim+1):\n",
        "  print(i)\n",
